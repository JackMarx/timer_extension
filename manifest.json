{
  "name": "Times URL",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Times URL",
  "background" : {
    "page" : "background.html",
    "persistent": false
  },
  "page_action" : {
    "default_icon": "icons/19.png"
  },
  "permissions": [
    "tabs",
    "http://andre-app.herokuapp.com/visits"
  ],
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "web_accessible_resources": [
    "settings/index.html"
  ],
  "options_page" : "settings/index.html"
}
