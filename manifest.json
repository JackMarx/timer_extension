{
  "name": "Times URL",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Times URL",
  "background" : {
    "page" : "background.html",
    "persistent": false
  },
  "page_action" :
  {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "web_accessible_resources": [
    "settings/index.html"
  ],
  "options_page" : "settings/index.html"
}
